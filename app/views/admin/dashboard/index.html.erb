<section id="dashboard">
  <p id="notice"><%= notice %></p>
  <h1>Dashboards</h1>
  <span style="float:right">
    <%= link_to 'New Dashboard', new_admin_dashboard_path, class: 'btn btn-primary' %>
  </span>
  <table>
    <thead>
      <tr><th colspan="3"></th></tr>
    </thead>

    <tbody>
      <% @admin_dashboards.each do |admin_dashboard| %>
        <tr>
          <td><%= link_to 'Show', admin_dashboard %></td>
          <td><%= link_to 'Edit', edit_admin_dashboard_path(admin_dashboard) %></td>
          <td><%= link_to 'Destroy', admin_dashboard, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
      <% end %>
    </tbody>
  </table>

  <br>

  

</section>
